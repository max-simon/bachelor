	Date-Modified = {2017-08-16 12:03:19 +0000},
	Journal = {Journal of Chemical Theory and Computation},
	Number = {9},
	Pages = {3257-3273},
	Title = {Optimization of the additive CHARMM all-atom protein force field targeting improved sampling of the backbone φ, ψ and side-chain χ 1 and χ 2 Dihedral Angles},
